`<h1>Listing openings</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Lat</th>
      <th>Long</th>
      <th>Requested</th>
      <th>Completed</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @openings.each do |opening| %>
      <tr>
        <td><%= opening.user_id %></td>
        <td><%= opening.Lat %></td>
        <td><%= opening.Long %></td>
        <td><%= opening.Requested %></td>
        <td><%= opening.Completed %></td>
        <td><%= link_to 'Show', opening %></td>
        <td><%= link_to 'Edit', edit_opening_path(opening) %></td>
        <td><%= link_to 'Destroy', opening, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Opening', new_opening_path %>
